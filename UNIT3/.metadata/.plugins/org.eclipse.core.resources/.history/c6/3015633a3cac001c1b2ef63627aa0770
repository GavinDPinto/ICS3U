
public class funWithLoops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Loop Fun");
		//loop types = while, for, do while
		
		int cnt = 0;
		while (cnt<5) {
			cnt++;
			System.out.println(cnt);
		}
		System.out.println();
		while (cnt>0) {
			System.out.println(cnt);
			cnt--;
		}
		System.out.println();
		do {
			cnt++;
			System.out.println(cnt);
		}while (cnt<5);
		System.out.println();
		do {
			System.out.println(cnt);
			cnt--;
		}while (cnt>0);
		System.out.println();
		for (int i = 0; i < 4; i++); {
//			System.out.println(1);
		}
		System.out.println();
		for (int i = 0; i < 4; i++) {
			for (int j = 0; j < 5; j++) {
				System.out.print((j + 1) + " ");
			}
			System.out.println();
		}
	}

}
